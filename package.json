{
  "name": "math-facts-fun",
  "version": "0.0.1",
  "description": "A free, open source tool for supporting early math and numeracy",
  "private": true,
  "dependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.13",
    "@types/react-beautiful-dnd": "^13.1.2",
    "firebase": "^8.10.1",
    "highcharts": "^10.2.0",
    "highcharts-react-official": "^3.1.0",
    "moment": "^2.29.3",
    "react": "16.13.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "16.13.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "5.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "AuthorizationInterfaces.tsx",
      "AuthorizationTypes.tsx",
      "CollectionTypes.tsx",
      "CommentTypes.tsx",
      "DashboardTypes.tsx",
      "FirebaseInterfaces.tsx",
      "LoginTypes.tsx",
      "ProgressTypes.tsx",
      "SetCreatorTypes.tsx",
      "RoutingInterfaces.tsx",
      "SetCreatorTypes.tsx",
      "SharedComponentTypes.tsx",
      "StudentInterfaces.tsx",
      "UserTypes.tsx"
    ]
  },
  "scripts": {
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-verbose": "npm run test -- --silent=false",
    "test-coverage": "npm run test -- --coverage --watchAll --coverageDirectory='coverage'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^12.8.3",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^29.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^8.23.0",
    "eslint-plugin-react": "^7.31.7",
    "identity-obj-proxy": "^3.0.0",
    "source-map-loader": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
